<!doctype html>
<html>
  <head>
  <meta charset="utf-8">
  <title>Jasmine Spec Runner</title>
    <% with (scripts) { %>
      <% [].concat(polyfills, jasmine, boot, vendor, src, helpers, specs, reporters).forEach(function(script){ %>
        <script src="<%= script %>"></script>
      <% }) %>
    <% }; %>
  </head>  
  <body>
    <div class="example">
      <div class="days"></div>
      <div class="hours"></div>
      <div class="minutes"></div>
      <div class="seconds"></div>
    </div>
    <div class="empty-example">
    </div>
    <div class="alt-example">
      <div class="days"></div>
      <div class="hours"></div>
      <div class="minutes"></div>
      <div class="seconds"></div>
    </div>
  </body>
</html>
